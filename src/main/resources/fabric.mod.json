{
	"schemaVersion": 1,
	"id": "superflat-recipes",
	"version": "${version}",
	"name": "Superflat Recipes",
	"description": "This is a mod that adds recipes for items that allow for further progression on superflat worlds; specifically structureless superflat.",
	"authors": [
		"AbsoluteXero"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/superflat-recipes/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.xero.superflat.SuperflatRecipes"
		],
		"client": [
			"net.xero.superflat.SuperflatRecipesClient"
		],
		"fabric-datagen": [
			"net.xero.superflat.SuperflatRecipesDataGenerator"
		]
	},
	"mixins": [
		"superflat-recipes.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}